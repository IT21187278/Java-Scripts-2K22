======== if else ===========

var marks=prompt("Enter your final marks ");
if (marks<100 && marks>80){
    alert('Congratulations!!!');
    alert("You have Grade A pass");
}
else if(marks<79 && marks>60){
    alert("You have Grade B pass");

}

else if(marks<59 && marks>30){
    alert("You have C pass");
}

else if(marks<30){
    alert('warnning !!!');
